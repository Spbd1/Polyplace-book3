{
  "name": "polyplace-root",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "server",
    "web"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev -w web\" \"npm run dev -w server\"",
    "clean": "rimraf node_modules server/node_modules package-lock.json server/package-lock.json",
    "reinstall": "npm run clean && npm i"
  },
  "overrides": {
    "@types/express": "4.17.25",
    "@types/express-serve-static-core": "4.19.7",
    "@types/connect": "3.4.38",
    "@types/cookie-parser": "1.4.7"
  },
  "devDependencies": {
    "@types/connect": "^3.4.38",
    "@types/express": "^4.17.25",
    "@types/express-serve-static-core": "^4.19.7",
    "concurrently": "^9.0.1"
  }
}